<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="jconon_titoli_ulteriori:jconon_titoli_ulterioriModel"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.alfresco.org/model/dictionary/1.0 modelSchema.xsd">
	<description>JCONON Dynamic Model</description>
	<author>marco.spasiano</author>
	<version>1.2</version>
	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
		<import uri="http://www.cnr.it/model/jconon_attachment/cmis" prefix="jconon_attachment" />
	</imports>
	<namespaces>
		<namespace uri="http://www.cnr.it/model/jconon_attachment/titoli_ulteriori/cmis"
			prefix="jconon_titoli_ulteriori" />
	</namespaces>
	<types>
		<type name="jconon_titoli_ulteriori:attachment">
			<title>Dichiarazione titoli ulteriori</title>
			<description>Dichiarazione titoli ulteriori</description>
			<parent>jconon_attachment:document_mono</parent>
			<properties>
				<property name="jconon_titoli_ulteriori:descrizione">
					<title>Descrizione</title>
					<description>Descrizione</description>
					<type>d:text</type>
					<mandatory>false</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>both</tokenised>
					</index>
				</property>
			</properties>
		</type>
	</types>
</model>